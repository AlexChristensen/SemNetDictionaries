citHeader("To cite SemNetDictionaries in publications use:")

citEntry(entry = "manual",
  title        = "{SemNetDictionaries}: Dictionaries for the {'SemNetCleaner'} package",
  author       = personList(as.person("Alexander P Christensen")),
  year         = "2019",
  note	       = "R package version 0.1.4",
  url          = "https://cran.r-project.org/package=SemNetDictionaries",

  textVersion  =
  paste("Christensen, A. P. (2019).",
        "SemNetDictionaries: Dictionaries for the 'SemNetCleaner' package.",
        "R package version 0.1.4.",
        "Retrieved from https://cran.r-project.org/package=SemNetDictionaries")
